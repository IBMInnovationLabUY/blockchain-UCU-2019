 './Spinners/SkewLoader.vue'\n// import SquareLoader from './Spinners/SquareLoader.vue'\nimport SyncLoader from './Spinners/SyncLoader.vue'\n\nconst componentsList = {\n  'beat-loader': BeatLoader,\n  // 'bounce-loader': BounceLoader,\n  'clip-loader': ClipLoader,\n  'dot-loader': DotLoader,\n  'fade-loader': FadeLoader,\n  // 'grid-loader': GridLoader,\n  'moon-loader': MoonLoader,\n  // 'pacman-loader': PacmanLoader,\n  'pulse-loader': PulseLoader,\n  // 'ring-loader': RingLoader,\n  'rise-loader': RiseLoader,\n  // 'rotate-loader': RotateLoader,\n  // 'scale-loader': ScaleLoader,\n  // 'skew-loader': SkewLoader,\n  // 'square-loader': SquareLoader,\n  'sync-loader': SyncLoader\n}\n\nexport default {\n  mounted () {\n    // console.log('IniLoadingSpinner ready!');\n  },\n  props: {\n    spinnerStyle: {\n      type: String,\n      default: 'rise-loader'\n      // default: 'beat-loader'\n      // default: 'dot-loader'\n      // default: 'sync-loader'\n      // default: 'pulse-loader'\n    },\n    loading: {\n      type: Boolean,\n      default: true\n    },\n    color: {\n      type: String,\n      // default: '#429bf4' // calm\n      default: '#5dc596' // green\n    },\n    size: {\n      type: String,\n      default: '25px'\n    },\n    margin: {\n      type: String,\n      default: '2px'\n    },\n    radius: {\n      type: String,\n      // default: '20px'\n      default: '100%'\n    }\n  },\n  data () {\n    var list = []\n    return {\n      notificationList: list,\n      showModal: false\n    }\n  },\n  components: componentsList,\n  methods: {\n    show: function () {\n      this.showModal = true\n    },\n    hide: function () {\n      this.showModal = false\n    }\n  }\n}\n</script>\n<style>\n.ini-spinner-mask {\n  position: fixed;\n  z-index: 9998;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(100, 100, 100, .1);\n  display: block;\n  transition: all .3s ease;\n}\n\n.ini-spinner-content {\n  width: 200px;\n  height: 100px;\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  margin: auto;\n  text-align: center;\n  display: block;\n  transition: all .3s ease;\n}\n\n.fade-spinner-enter {\n  opacity: 0;\n}\n\n.fade-spinner-leave-active {\n  opacity: 0;\n}\n\n.fade-spinner-enter .ini-spinner-content,\n.fade-spinner-leave-active .ini-spinner-content {\n  -webkit-transform: scale(1.1);\n  transform: scale(1.1);\n}\n</style>\n"]}]}